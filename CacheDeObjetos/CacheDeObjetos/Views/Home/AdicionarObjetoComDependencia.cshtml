@model CacheDeObjetos.Models.Cotacao
@{
    ViewBag.Title = "";
}

<h2>Objeto em Cache com dependência</h2>
<em>Adiciona o objeto em cache que altera quando sua dependência é alterada</em>

<p>Descrição: @Model.Descricao</p>
<p>Valor: @Model.Valor.ToString("0.##")</p>
<p>Ultima Atualização: @Model.UltimaAtualizacao.ToString("dd/MM/yyyy HH:mm:ss")</p>

<p>@Html.ActionLink("Alterar dependência", "AlteraArquivoDeDependencia", "Home")</p>
